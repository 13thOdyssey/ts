-- Scripts:

local function VLVZTJB_fake_script() -- fov.LocalScript 
	local script = Instance.new('LocalScript', fov)

	script.Parent.MouseButton1Click:Connect(function()
	
		game.Workspace.CurrentCamera.FieldOfView  = 100
	end)
end
coroutine.wrap(VLVZTJB_fake_script)()
local function NCPUF_fake_script() -- resetkey.LocalScript 
	local script = Instance.new('LocalScript', resetkey)

	script.Parent.MouseButton1Click:Connect(function()
	
		wait(2)
		print("reset by Kutsukie has loaded")
		game.Players.LocalPlayer:GetMouse().KeyDown:Connect(function(KeyPressed)
			if KeyPressed == "x" then
				game.Players.LocalPlayer.Character.Humanoid.Health = 0
			end
		end)
	end)
end
coroutine.wrap(NCPUF_fake_script)()
local function CJHYGHJ_fake_script() -- CloseButton.Script 
	local script = Instance.new('Script', CloseButton)

	frame = script.Parent.Parent
	button = script.Parent
	
	button.MouseButton1Click:connect(function()
		frame.Visible = false
	end)
end
coroutine.wrap(CJHYGHJ_fake_script)()
local function OLJDBCY_fake_script() -- Frame.LocalScript 
	local script = Instance.new('LocalScript', Frame)

	local b = script.Parent.TextButton
	local f = script.Parent.Frame
	local cb = script.Parent.Frame.TextButton
	
	b.MouseButton1Click:Connect(function()
		f:TweenPosition(UDim2.new(0.231, 0,0.214,0),"Quint","Quint")
		
	end)
end
coroutine.wrap(OLJDBCY_fake_script)()
local function BQZPL_fake_script() -- fov_2.LocalScript 
	local script = Instance.new('LocalScript', fov_2)

	script.Parent.MouseButton1Click:Connect(function()
	
		game:GetService("TeleportService"):Teleport(455366377, game:GetService("Players").LocalPlayer)
	end)
end
coroutine.wrap(BQZPL_fake_script)()
local function FONES_fake_script() -- TextButton.Script 
	local script = Instance.new('Script', TextButton)

	button = script.Parent
	frame = script.Parent.Parent.Parent.OpenTSKit.Frame
	
	button.MouseButton1Click:connect(function()
		frame.Visible = true
	end)
	
end
coroutine.wrap(FONES_fake_script)()
local function KIGTT_fake_script() -- OpenTSKit.LocalScript 
	local script = Instance.new('LocalScript', OpenTSKit)

	local Frame = script.Parent.Frame
	
	Frame.Draggable = true
	Frame.Active = true
end
coroutine.wrap(KIGTT_fake_script)()
